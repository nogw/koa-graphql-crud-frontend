    "relay:compile": "relay-compiler --src ./src --schema ./src/data/schema.graphql --language typescript",
    "relay:schema": "get-graphql-schema http://localhost:4000/graphql > ./src/data/schema.graphql"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.5",
